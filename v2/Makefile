help:
	@echo "=============== BLOGGER ================="
	@echo "help ................ Show Help"
	@echo "install ............. Install Blogger"
	@echo "run ................. Run Blogger"

install:
	@echo Running make install...

run:
	@echo Running make run...

# to catch all default targets and do nothing
.DEFAULT: ;

.PHONY: help install run
