help:
	@echo "=============== BLOGGER ================="
	@echo "help ................ Show Help"
	@echo "exec ................ Execute shell command in container"
	@echo "clean ............... Clean Installation"

exec:
	@echo $(CMD)
	@$(CMD)

clean:
	rm -rf node_modules

# to catch all default targets and do nothing
.DEFAULT: ;

.PHONY: help exec clean
